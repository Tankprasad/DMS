@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutSuperAdmin.cshtml";
}

<div class="row">
    <div class="col-sm-12">
        <div class="input-group input-group-sm">
            <input type="text" class="form-control" id="branchNameId" onkeypress="searchFunction(' ')">
            <span class="input-group-btn">
                <button type="button" class="btn btn-info btn-flat" onclick="searchFnction(' ')">Search</button>
            </span>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-sm-6" style="text-align:left">
        <div id="form_errors"></div>
    </div>
</div>
<div class="table table-responsive">


    <div id="branchList"></div>

</div>
@*<link href="~/Content/AdminLTE.css" rel="stylesheet" />
    <script src="~/Scripts/CustomScripts/globalFunction.js"></script>*@

<div class="modal fade" id="add-branch" tabindex="-1" role="dialog" aria-labelledby="addBranch">
    @{Html.RenderAction("Create", "SetupBranch");}
</div>

<div class="modal fade" id="update-branch" tabindex="-1" role="dialog" aria-labelledby="editBranch">
    @{Html.RenderAction("Edit", "SetupBranch");}
</div>

<div class="modal fade" id="delete-conformation" tabindex="-1" role="dialog" aria-labelledby="deleteconformation">
    @{Html.RenderAction("DeleteConformation", "CommonAjaxRequest", new { area = "" });}
</div>


<script>
    $(document).ready(function (e) {
        search($('#branchNameId').val(), '', '#branchList', '_GetBranchList', 'SetupBranch', 'SetupArea');
    });
</script>

